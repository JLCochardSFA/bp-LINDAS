# Needed to generate the relation between a shared dimension and a cube using it

PREFIX dms: <https://schema.ld.admin.ch/datamngt#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX dcat: <http://www.w3.org/ns/dcat#>
PREFIX sfa: <https://lindas.admin.ch/sfa/>
PREFIX blueprintConfig: <https://described.at/zazuko/blueprintConfig/>
PREFIX flux: <https://flux.described.at/>
PREFIX sh: <http://www.w3.org/ns/shacl#>
PREFIX schema: <http://schema.org/>
PREFIX cube: <https://cube.link/>
PREFIX cube-meta: <https://cube.link/meta/>

CONSTRUCT {
    GRAPH sfa:dataMngt {
        dms:isUsedIn    a rdf:Property;
                        rdfs:domain dms:SharedDimension;
                        rdfs:range dcat:Cube.
        ?sharedDimension dms:isUsedIn ?cube
    }
}
# SELECT ?sharedDimension ?name ?cube 
WHERE {
    ?sharedDimension 
        a cube-meta:SharedDimension;
        cube-meta:isUsedIn ?cube.
}
